               <div class="form-group">
                 <input class="form-control" placeholder="Phone Number" type="number" name="user_phone">
               </div>
               <div class="form-group">
                 <input class="form-control" placeholder="Email ID" type="email" name="user_email">
               </div>
               <button type="submit" class="btn btn-lg rounded-pill btn-res" name="submit" style="background-color: #2B9DB6;">Book Now</button>
             </form>
             </div>
             </div>
             <div class="modal-footer">
                 <button type="button" class="btn btn-secondary btn-res" data-dismiss="modal">Close</button>
