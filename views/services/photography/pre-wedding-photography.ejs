   <!-- Button trigger modal -->
   <div class="text-center">
   <button type="button" class="btn btn-lg rounded-pill btn-res" style="background-color: #2B9DB6;" data-toggle="modal" data-target="#exampleModal">
     Book Now
   </button>
   </div>
   
   <!-- Modal -->
   <div class="modal fade modal-fullscreen" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
     <div class="modal-dialog" role="document">
         <div class="modal-content">
             <div class="modal-header">
                 <h5 class="modal-title" id="exampleModalLabel">Fill Your Details</h5>
                 <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                     <span aria-hidden="true">&times;</span>
                 </button>
             </div>
             <div class="modal-body">
               
           <!-- FORM ---------------------------->
           <div class="container">
             <form action="/services/photography/wedding-photography" method="POST">
   
               <div class="" style="display: none;">
               <input type="text" value="Pre Wedding Photography" name="user_service">
               </div>
   
               <div class="form-group">
                 <input class="form-control" placeholder="Full Name" type="text" name="user_name">
               </div>
               <div class="form-group">
                 <input class="form-control" placeholder="Phone Number" type="number" name="user_phone">
               </div>
               <div class="form-group">
                 <input class="form-control" placeholder="Email ID" type="email" name="user_email">
               </div>
               <button type="submit" class="btn btn-lg rounded-pill btn-res" name="submit" style="background-color: #2B9DB6;">Book Now</button>
             </form>
             </div>
             </div>
             <div class="modal-footer">
                 <button type="button" class="btn btn-secondary btn-res" data-dismiss="modal">Close</button>
             </div>
         </div>
     </div>
   </div>

